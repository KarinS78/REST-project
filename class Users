 class Users:
    def __init__(self, id_AI, full_name, password, real_id):
        self.id_AI = id_AI
        self.full_name = full_name
        self.password = password
        self.real_id = real_id

    def __repr__(self):
        return f'Users({self.id_AI},\'{self.full_name}\', \'{self.password}\', \'{self.real_id}\')'
